<%= simple_form_for [:users, vessel], defaults: { wrapper_html: { class: 'form-group' }, input_html: { class:'form-control' } }, html: { multipart: true, novalidate: true } do |f| %>
  <%= f.input :name %>
  <%= f.input :code %>
  <%= f.input :reference_number %>
  <%= f.submit "Save", class: "btn btn-primary" %>
<% end %>
